generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("NETLIFY_DATABASE_URL")
  directUrl = env("NETLIFY_DATABASE_URL_UNPOOLED")
}

model RandomNumber {
  id        Int      @id @default(autoincrement())
  value     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
